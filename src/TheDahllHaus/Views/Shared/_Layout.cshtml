<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="//cdn.muicss.com/mui-0.9.24/css/mui.min.css" rel="stylesheet" type="text/css" />
    <script src="//cdn.muicss.com/mui-0.9.24/js/mui.min.js"></script>
    <script src="//code.jquery.com/jquery-2.1.4.min.js"></script>

    <!--local and non MUI CDNs-->
    <link href="https://fonts.googleapis.com/css?family=PT+Sans" rel="stylesheet">
    <link href="~/css/main.css" rel="stylesheet" />
    <script src="~/js/scripts.js"></script>
    <script src="https://use.fontawesome.com/c3a95916f1.js"></script>
    <title>@ViewBag.Title</title>
</head>

<body>
    <div id="sidedrawer" class="mui--no-user-select">
        <div id="sidedrawer-brand" class="mui--appbar-line-height">
            <span class="mui--text-title">Brand.io</span>
        </div>
        <div class="mui-divider"></div>
        <ul>
            <li>
                <strong>About</strong>
                <ul>
                    <li><a href="#">Meet the Artist</a></li>
                    <li><a href="#">Contact</a></li>
                    <li><a href="#">Reviews</a></li>
                </ul>
            </li>
            <li>
                <strong>Custom Creations</strong>
                <ul>
                    <li><a href="#">Headdresses, Fascinators and Hats</a></li>
                    <li><a href="#">Masks</a></li>
                    <li><a href="#">Costumes</a></li>
                    <li><a href="#">Wigs</a></li>
                    <li><a href="#">Zines</a></li>
                </ul>
            </li>
            <li>
                <strong>MUA</strong>
                <ul>
                    <li><a href="#">Hair</a></li>
                    <li><a href="#">Makeup</a></li>
                    <li><a href="#">Production Assistant</a></li>
                </ul>
            </li>
        </ul>
    </div>
    <header id="header">
        <div class="mui-appbar mui--appbar-line-height">
            <div class="mui-container-fluid">
                <a class="sidedrawer-toggle mui--visible-xs-inline-block mui--visible-sm-inline-block js-show-sidedrawer">☰</a>
                <a class="sidedrawer-toggle mui--hidden-xs mui--hidden-sm js-hide-sidedrawer">☰</a>
                <span class="mui--text-title mui--visible-xs-inline-block">
                    <img src="~/images/kateslogos/thedahllhouse_logo_white.png" width="250"/>
                </span>
            </div>
        </div>
    </header>
    <div id="content-wrapper">
        @RenderBody()
    </div>
    <footer id="footer">
        <div class="mui-container-fluid">
            <br>
            Made with ♥ by <a href="https://www.muicss.com">MUI</a>
        </div>
    </footer>
</body>
</html>
